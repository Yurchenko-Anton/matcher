<?xml version="1.0" encoding="utf-8" ?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">


    <changeSet id="6" author="Yuar">
        <createTable tableName="drivers_place">
            <column name="id" type="int">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="driver_id" type="int"/>
            <column name="place_id" type="int"/>
        </createTable>

        <addAutoIncrement tableName="drivers_place" columnName="id" columnDataType="int" startWith="1" incrementBy="1"/>
        <addForeignKeyConstraint baseTableName="drivers_place" baseColumnNames="driver_id" constraintName="driver_users_id_fk" referencedTableName="users"
                                 referencedColumnNames="id"/>
        <addForeignKeyConstraint baseTableName="drivers_place" baseColumnNames="place_id" constraintName="place_driver_id_fk" referencedTableName="places"
                                 referencedColumnNames="id"/>
    </changeSet>
</databaseChangeLog>